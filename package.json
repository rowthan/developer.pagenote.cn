{
  "version": "0.26.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "move-ext": "mv pages _pages && mkdir pages && cp -rf _pages/ext pages/ext && cp _pages/_app.tsx pages/_app.tsx",
    "revert-pages": "rm -rf pages && mv _pages pages",
    "build-dev-ext": "prefix=/web next build && next export",
    "build-ext": "npm run move-ext && prefix=/web next build && next export && npm run revert-pages",
    "start": "next start",
    "lint-fix": "next lint --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "npm run lint-fix"
    ]
  },
